
public class Main {

	public static void main(String[] args) {
		Conta cc = new ContaCorrente();
		Conta poupanca = new ContaPoupanca();
		
		cc.depositar(100); //deposita $100 na cc
		cc.transferir(100, poupanca); //transfere $100 para a poupan√ßa
		
		cc.imprimirExtrato();
		poupanca.imprimirExtrato();
	}

}
